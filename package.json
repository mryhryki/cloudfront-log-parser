{
  "name": "cloudfront-log-parser",
  "author": "mryhryki",
  "private": true,
  "scripts": {
    "build": "esbuild --bundle src/index.ts --outfile=index.js --minify --platform=node --target=node14",
    "check": "tsc --noEmit",
    "init": "test -f src/config.ts || cp src/config{.template,}.ts",
    "run": "node index.js",
    "start": "run-s init check build run"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^17.0.36",
    "@types/node-gzip": "^1.1.0",
    "aws-sdk": "^2.1145.0",
    "esbuild": "^0.14.42",
    "node-gzip": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.2"
  }
}
